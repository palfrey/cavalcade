{
    "problemMatcher": [
        {
            "owner": "rust-panic",
            "pattern": [
                {
                    "regexp": "thread '.+' panicked at '(.+)', /work/([^:]+):(\\d+):(\\d+)",
                    "file": 2,
                    "line": 3,
                    "column": 4,
                    "message": 1
                }
            ]
        }
    ]
}
