{
    "problemMatcher": [
        {
            "owner": "rust-panic",
            "pattern": [
                {
                    "regexp": "thread '.+' panicked at '(.+)', ([^:]+):(\\d+):(\\d+)",
                    "file": 3,
                    "line": 4,
                    "column": 5,
                    "severity": 3,
                    "message": 2
                }
            ]
        }
    ]
}